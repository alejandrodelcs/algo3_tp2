@startuml
'https://plantuml.com/class-diagram

package model {
    class Health {
        - healthPoints: int
        + Health(int healthPoints)
        + takeDamage(int damage): void
        + entityDied(): boolean
        + isLowerOrEqualThan(Health compareintHealth): boolean
        + isGreaterThan(int HealthPoints): boolean
        + getHealthPoints(): int
    }
}

@enduml