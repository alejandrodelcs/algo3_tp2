@startuml
'https://plantuml.com/class-diagram

package model {
    class Defense {
      - credits: Credit
      - coordinates: Point
      - state: State
      - attack: Attack
      + getCredits(): Credit
      + getPoint(): Point
      + attack(enemies: ArrayList<Enemy>): void
      + constructionFinished(): void
      + destructed(): void
      + show(): String
      + isAvailable(): boolean
      + isDestroyed(): boolean
      + getPlotsInRange(): ArrayList<Point>
    }
}

@enduml