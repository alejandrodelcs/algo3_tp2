@startuml
'https://plantuml.com/class-diagram
skinparam linetype ortho
!include Plot.puml
!include PlotAvailability.puml
!include NotPlotAvailability.puml
!include Occupied.puml
!include Available.puml
!include Dirt.puml
!include Stone.puml
!include Path.puml
!include FinishLine.puml

